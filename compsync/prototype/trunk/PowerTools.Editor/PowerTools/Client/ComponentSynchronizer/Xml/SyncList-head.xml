<?xml version="1.0"?>
<list:listDefinition xmlns:list="http://www.sdltridion.com/2009/GUI/extensions/List">
  <list:selectornamespaces>
    <list:namespace prefix="tcm" uri="http://www.tridion.com/ContentManager/5.0"/>
  </list:selectornamespaces>
  <list:rows>
    <list:itemSelector>/tcm:ListUsingItems/tcm:Item</list:itemSelector>
    <list:titleSelector>@Title</list:titleSelector>
    <list:idSelector>@ID</list:idSelector>
    <list:emptyListMessage>No items found for synchronization</list:emptyListMessage>
  </list:rows>
  <list:columns>
    <list:column id="IconColum" type="icon" title="" width="20" enableresizing="false" enablesorting="false" enablefiltering="false" selector="@Icon" />
    <list:column id="NameColumn" type="data" title="{Resources: Tridion.Web.UI.Strings, Name}"  selector="@Title"  enablefiltering="true" columnfilter="group"/>
    <list:column id="NameColumn" type="data" title="Synchronization Status" width="300" selector="@SyncStatus"  enablefiltering="true" columnfilter="group"/>
    <!--list:column id="PublicationColumn" type="data" title="{Resources: Tridion.Web.UI.Strings, Publication}" selector="@Publication"  enablefiltering="true" columnfilter="value"/>
		<list:column id="CommentColumn" type="data" title="{Resources: Tridion.Web.UI.Strings, Comment}" selector="@Comment"  enablefiltering="true" columnfilter="value"/>
		<list:column id="LocationColumn" type="data" title="{Resources: Tridion.Web.UI.Strings, Location}" selector="@Path"  enablefiltering="true" columnfilter="group"/ -->
  </list:columns>
</list:listDefinition>
