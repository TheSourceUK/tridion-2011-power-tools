          <cfg:file type="script">/PowerTools/Client/Shared/Scripts/Types.js</cfg:file>
          <cfg:file type="script">/PowerTools/Client/Shared/Scripts/PowerToolsRegisty.js</cfg:file>
          <cfg:file type="script">/PowerTools/Client/Shared/Scripts/ToolBase.js</cfg:file>
        </cfg:fileset>
      </cfg:group>

      <!-- Theme files (based on the CME theme)-->
      <cfg:group name="PowerTools.ThemeFiles">
        <cfg:fileset>
          <cfg:file type="style">{ThemePath}/styles.css</cfg:file>
        </cfg:fileset>
        <cfg:dependencies>
          <cfg:dependency>Tridion.Web.UI.Editors.CME.Theme</cfg:dependency>
        </cfg:dependencies>
      </cfg:group>

      <!-- Show publish tasks on PublishQueue open-->
      <cfg:group name="PowerTools.ShowPublishTaskOnOpen">
        <cfg:fileset>
          <cfg:file type="script">/PowerTools/Client/ShowPublishTasks/ShowPublishTasks.js</cfg:file>
        </cfg:fileset>
      </cfg:group>

      <!-- XML Syntax Highlighter Tab -->
      <cfg:group name="PowerTools.ItemXmlTab" merge="always">
