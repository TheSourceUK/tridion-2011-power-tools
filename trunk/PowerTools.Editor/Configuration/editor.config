            <ext:extension assignid="ViewInStagingBtn" name="View In Staging"  pageid="Power Tools" groupid="Tools">
              <ext:command>PT_ViewInStaging</ext:command>
              <ext:title>View In Staging</ext:title>
              <ext:apply>
                <ext:view name="DashboardView" />
              </ext:apply>
            </ext:extension>

            <ext:extension assignid="ViewInLiveBtn" name="View In Live" pageid="Power Tools" groupid="Tools">
              <ext:command>PT_ViewInLive</ext:command>
              <ext:title>View In Live</ext:title>
              <ext:apply>
                <ext:view name="DashboardView" />
              </ext:apply>
            </ext:extension>

            <!-- Help -->
            <ext:extension assignid="Help" name="PowerTools&lt;br /&gt;Help" pageid="Power Tools" groupid="Tools">
              <ext:command>PT_Help</ext:command>
              <ext:title>Help</ext:title>
              <ext:dependencies>
                <cfg:dependency>PowerTools.Commands</cfg:dependency>
              </ext:dependencies>
              <ext:apply>
                <ext:view name="DashboardView" />
              </ext:apply>
            </ext:extension>

          </ext:add>
        </ext:ribbontoolbars>
      </ext:editorextension>
    </ext:editorextensions>
    <ext:dataextenders/>
  </extensions>

  <commands>
    <cfg:commandset id="PowerTools.Commandset">
      <cfg:command name="PT_Index" implementation="PowerTools.Commands.Index"/>
      <cfg:command name="PT_Example" implementation="PowerTools.Commands.Example"/>
      <cfg:command name="PT_ImageUploader" implementation="PowerTools.Commands.ImageUploader"/>
      <cfg:command name="PT_PagePublisher" implementation="PowerTools.Commands.PagePublisher"/>
      <cfg:command name="PT_FieldRemover" implementation="PowerTools.Commands.FieldRemover"/>
      <cfg:command name="PT_MarkUnpublished" implementation="PowerTools.Commands.MarkUnpublished"/>
      <cfg:command name="PT_ComponentSynchronizer" implementation="PowerTools.Commands.ComponentSynchronizer"/>
      <cfg:command name="PT_CountItems" implementation="PowerTools.Commands.CountItems"/>
      <cfg:command name="PT_AppDataInspector" implementation="PowerTools.Commands.AppDataInspectorPopup"/>
      <cfg:command name="PT_ViewInStaging" implementation="PowerTools.Commands.ViewInStaging"/>
      <cfg:command name="PT_ViewInLive" implementation="PowerTools.Commands.ViewInLive"/>
      <cfg:command name="PT_DocumentItem" implementation="PowerTools.Commands.DocumentItem"/>
      <cfg:command name="PT_Help" implementation="PowerTools.Commands.Help"/>
      <cfg:dependencies>
        <cfg:dependency>PowerTools.Resources.Base</cfg:dependency>
      </cfg:dependencies>
    </cfg:commandset>
  </commands>

  <contextmenus/>

  <localization/>

  <settings>
    <defaultpage>/Views/Default.aspx</defaultpage>

    <navigatorurl>/Views/Default.aspx</navigatorurl>

    <editurls/>

    <listdefinitions/>

    <itemicons/>

    <theme>
      <path>/PowerTools/Client/Shared/Theme/</path>
      <resourcegroup>PowerTools.ThemeFiles</resourcegroup>
    </theme>
    <resourceextensions>
      <resourceextension>ShowPublishTaskOnOpen</resourceextension>
    </resourceextensions>
    <customconfiguration>
      <listofpowertools>
        <pt id="AppDataInspector" category="Developers">AppData Inspector</pt>
        <pt id="Example" category="Developers">Example PowerTool</pt>
        <pt id="ComponentSynchronizer" category="System">Component Synchronizer</pt>
        <pt id="CountItems" category="General">Count Items</pt>
        <pt id="DocumentItem" category="Developers">Document Item</pt>
        <pt id="FieldRemover" category="System">Field remover</pt>
        <pt id="Help" category="General">Help tool</pt>
        <pt id="ImageUploader" category="General">Imageuploader</pt>
        <pt id="ItemXml" category="Developers">ItemXml</pt>
        <pt id="MarkUnpublish" category="System">Mark as Unpublished</pt>
        <pt id="PagePublisher" category="General">Page publisher</pt>
        <pt id="PublishQueueViewExtension" category="General">Show publish tasks</pt>
        <pt id="ViewIn" category="General">View in frontend</pt>
      </listofpowertools>
    </customconfiguration>
  </settings>
</Configuration>

