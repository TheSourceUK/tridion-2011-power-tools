              </ext:apply>
            </ext:extension>

            <!-- Component Synchronizer PowerTool -->
            <ext:extension assignid="ComponentSynchronizer" name="Component&lt;br/&gt;Synchronizer" pageid="Power Tools" groupid="Tools">
              <ext:command>PT_ComponentSynchronizer</ext:command>
